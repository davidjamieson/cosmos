{
	"name": "ll-cosmos",
	"version": "1.0.0",
	"private": true,
	"description": "Living Lab COSMOS",
	"main": "main.js",
	"scripts": {
		"cosmos-dev": "npm run cosmos-development",
		"cosmos-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.mix.cosmos --config=node_modules/laravel-mix/setup/webpack.config.js",
		"cosmos-watch": "npm run cosmos-development -- --watch",
		"cosmos-prod": "npm run cosmos-production",
		"cosmos-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --env.mixfile=webpack.mix.cosmos --config=node_modules/laravel-mix/setup/webpack.config.js",
		"admin-dev": "npm run admin-development",
		"admin-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.mix.admin --config=node_modules/laravel-mix/setup/webpack.config.js",
		"admin-watch": "npm run admin-development -- --watch",
		"admin-prod": "npm run admin-production",
		"admin-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --env.mixfile=webpack.mix.admin --config=node_modules/laravel-mix/setup/webpack.config.js"
	},
	"author": "Newcastle University",
	"license": "MIT",
	"devDependencies": {
		"axios": "^0.19.0",
		"colors.css": "^3.0.0",
		"cross-env": "^6.0.3",
		"date-fns": "^2.15.0",
		"es6-promise": "^4.2.8",
		"jspdf": "^1.5.3",
		"konva": "^4.0.18",
		"laravel-mix": "^4.1.4",
		"lodash": "^4.17.15",
		"md5": "^2.2.1",
		"resolve-url-loader": "^3.1.0",
		"sass": "^1.22.12",
		"sass-loader": "^7.3.1",
		"spectre.css": "^0.5.8",
		"vue": "^2.6.10",
		"vue-feather-icons": "^5.0.0",
		"vue-i18n": "^8.15.1",
		"vue-konva": "^2.1.1",
		"vue-router": "^3.1.3",
		"vue-slicksort": "^1.1.3",
		"vue-template-compiler": "^2.6.10",
		"vue-tour": "^1.5.0",
		"vuex": "^3.1.2",
		"vuex-map-fields": "^1.3.6",
		"vuex-pathify": "^1.4.1",
		"@newcastle-living-lab/living-lab-admin": "^1.0.0"
	},
	"dependencies": {
		"connect-history-api-fallback": "^1.6.0"
	}
}
